    <template id="ROUTE">
        <description>导航</description>
        <pattern type="EXACT">^(【我】)?(【想】)?【导航】$</pattern>
        <semantics>
            <domain>map</domain>
            <intent>route</intent>
        </semantics>
    </template>

     <template id="ROUTE_ARRIVAL">
        <description>导航去百度大厦</description>
        <pattern type="FUZZY">【往】 【loc】</pattern>
        <pattern type="FUZZY">【目的地】 【loc】</pattern>
        <pattern type="FUZZY">目的 地 【loc】</pattern>
        <pattern type="FUZZY">【loc】 【如何】 【去】</pattern>
        <pattern type="FUZZY">【如何】 【去】 【loc】</pattern>
        <pattern type="FUZZY">【loc】 【从】 【哪里】 【去】</pattern>
        <pattern type="FUZZY">【loc】 【在】 【个】 【哪里】</pattern>
        <pattern type="FUZZY">【loc】 地图 中 【的】 【位置】</pattern>
        <pattern type="FUZZY">【loc】 【导航】 【route_type】 【路线】</pattern>
        <pattern type="FUZZY">【loc】 【导航】 【路线】</pattern>
        <pattern type="FUZZY">【loc】 【在】 【个】 方向</pattern>
        <pattern type="FUZZY">【loc】 【往】 【哪里】 【去】</pattern>
        <pattern type="FUZZY">【loc】 快捷 【导航】 【路线】</pattern>
        <pattern type="FUZZY">【route_type】 【去】 【loc】</pattern>
        <pattern type="FUZZY">【loc】 【如何】 【route_type】</pattern>
        <pattern type="EXACT">^【去】 【loc】$</pattern>
        <pattern type="EXACT">^【loc】 【路线】$</pattern>
        <pattern type="FUZZY">【loc】 【在】 【哪里】</pattern>
        <pattern type="FUZZY">【loc】 【有】 多 远</pattern>
        <semantics>
          <domain>map</domain>
          <intent>route_arrival</intent>
        </semantics>
      </template>
      
    <template id="ROUTE_POITYPE">
        <description>附近的电影院</description>
        <pattern type="LOCAL">【去】【poi_type】</pattern>
        <semantics>
          <domain>map</domain>
          <intent>route_poitype</intent>
        </semantics>
    </template>
    <template id="NEARBY_POITYPE">
        <description>附近的电影院</description>
        <pattern type="EXACT">^【搜索】 【个】 【poi_type】$</pattern>
        <pattern type="LOCAL">【搜索】【poi_type】</pattern>
        <semantics>
          <domain>map</domain>
          <intent>nearby_poitype</intent>
        </semantics>
    </template>
    <template id="NEARBY_LOCATION_POITYPE">
        <description>百度大厦附近的电影院</description>
        <pattern type="LOCAL">【loc】【附近】【poi_type】</pattern>
        <pattern type="LOCAL">【去】【loc】【poi_type】</pattern>
        <pattern type="EXACT">【loc】 【哪里】 【有】 【poi_type】</pattern>
        <pattern type="LOCAL">【导航】【loc】【poi_type】</pattern>
        <pattern type="LOCAL">【loc】【poi_type】</pattern>
        <semantics>
          <domain>map</domain>
          <intent>nearby_location_poitype</intent>
        </semantics>
    </template>
    <template id="ROUTE_START_ARRIVAL">
        <description>从百度大厦到上地地铁站</description>
        <pattern type="FUZZY">【loc】【出发】【去】【poi_type】</pattern>
        <pattern type="FUZZY">【loc】【去】【poi_type】</pattern>
        <pattern type="FUZZY">【从】 【loc】 【去】 【loc】 最 快 【路线】</pattern>
        <pattern type="FUZZY">【从】 【loc】 【去】 【loc】 最 短 【路线】</pattern>
        <pattern type="EXACT">^【从】 【loc】 【去】 【loc】$</pattern>
        <pattern type="FUZZY">【从】 【loc】 【去】 【loc】</pattern>
        <pattern type="FUZZY">【从】 【loc】 【去】 【loc】 【的】 最佳 【路线】</pattern>
        <pattern type="FUZZY">【从】 【loc】 【去】 【loc】 【导航】</pattern>
        <semantics>
          <domain>map</domain>
          <intent>route_start_arrival</intent>
        </semantics>
    </template>
    <template id="ROUTE_HERE_ARRIVAL">
        <description>从这到上地地铁站</description>
        <pattern type="EXACT">^【从】【这】【去】【loc】【啊】?$</pattern>
        <pattern type="FUZZY">【从】 【我】 【这】 【如何】 【去】 【loc】</pattern>
        <pattern type="FUZZY">【从】 【这】 【去】 【loc】</pattern>
        <pattern type="FUZZY">【去】 【loc】</pattern>
        <semantics>
          <domain>map</domain>
          <intent>route_here_arrival</intent>
        </semantics>
    </template>
    <template id="ROUTE_ARRIVAL_START">
        <description>到上地地铁站从百度大厦走</description>
        <pattern type="FUZZY">【去】【loc】【从】【location1】</pattern>
        <semantics>
          <domain>map</domain>
          <intent>route_arrival_start</intent>
        </semantics>
    </template>
</templates>
